{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "ecmaFeatures": {
    "modules": true,
    "spread" : true,
    "restParams" : true
  },
  "rules": {
    "import/no-extraneous-dependencies": [0],
    "no-console": [0],
    "no-underscore-dangle": [0],
    "react/jsx-filename-extension": [0],
    "no-unused-vars" : 2,
    "no-undef" : 2
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
